<% if( typeof(jobs) !='undefined' && jobs !=null ) { %>
    
    <div class="row">
        <div class="col-lg-12">
            <table width="100%" class="table" id="dataTables-example">
                <thead>
                    <tr>
                        <th>Area</th>
                        <th>Institution</th>
                        <th>Level</th>
                        <th>Options</th>
                    </tr>
                </thead>
                <tbody>
                <% for(var i =0; i< jobs.length; i++ ) { %>
                        <tr class="odd gradeX">
                            <td><div class="panel-body"><%= jobs[i].title %></div></td>
                            <td><a href="/teachers/applyjob/<%=jobs[i].id %>" class="btn btn-success">Apply</a></td>
                        </tr>
                        
                <% } %>
                </tbody>
            </table>
        </div>
    </div>
<% } %>

